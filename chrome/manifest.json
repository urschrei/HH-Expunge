{
  "name": "HH-Expunge",
  "version": "1.0",
  "homepage_url": "https://github.com/urschrei/HH-Expunge",
  "icons": { "48": "Icon.png"},
  "description": "Expunges HumHum posts from users you wish to ignore",
  "background_page": "background.html",
  "content_scripts": [
  {
    "matches": [
    "http://humhum.be/*/*.html*",
    "http://www.humhum.be/*/*.html*"
    ],
    "js" : ["chrome_script.js"]
  }
  ],
  "options_page": "options.html",
  "permissions": [
  "tabs",
  "http://www.humhum.be/*",
  "http://humhum.be/*"
  ],
  "update_url": "http://github.com/urschrei/HH-expunge/blob/master/chrome/updates.xml"
}