{
  "name": "HH-Expunge",
  "version": "1.2",
  "homepage_url": "https://github.com/urschrei/HH-Expunge",
  "icons": { "48": "Icon.png"},
  "description": "Expunges HumHum posts from users you wish to ignore",
  "background_page": "background.html",
  "content_scripts": [
  {
    "matches": [
    "http://humhum.be/*/*.html*",
    "http://www.humhum.be/*/*.html*"
    ],
    "js" : ["chrome_script.js"]
  }
  ],
  "options_page": "options.html",
  "permissions": [
  "tabs",
  "http://www.humhum.be/*",
  "http://humhum.be/*"
  ],
  "page_action": {
    "default_icon": "Icon_19px.png",
    "default_title": "Posts on this page have been hidden"
  },
  "update_url": "https://github.com/downloads/urschrei/HH-Expunge/updates.xml"
}